{
	"arrowParens": "always",
	"bracketSameLine": false,
	"bracketSpacing": true,
	"endOfLine": "lf",
	"printWidth": 100,
	"quoteProps": "consistent",
	"semi": true,
	"singleAttributePerLine": true,
	"tabWidth": 4,
	"trailingComma": "es5",
	"useTabs": true,
	"plugins": ["prettier-plugin-astro"],
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	]
}
